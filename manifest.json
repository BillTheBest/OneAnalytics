{
  "name": "OneAnalytics",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A darker Google Analytics",
  "homepage_url": "https://github.com/MehediH/OneAnalytics",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://analytics.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://analytics.google.com/*"
      ],
      "css": [
        "src/analytics.css"
      ]
    }
  ]
}